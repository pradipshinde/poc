<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	
	
	<context:annotation-config></context:annotation-config>
	
	<bean id="collegeBean" class="com.pradip.spring.autowiredandqualifier.College"> 
	<property name="collegename" value="Pradip"></property>
	<property name="collegeaddress" value="Pimpri"></property>
	</bean>
	
	
	<bean id="student1" class="com.pradip.spring.autowiredandqualifier.Student">
	<property name="rollno" value="44"></property>
	<property name="name" value="Pradip"></property>
	<property name="address" value="Kurduwadi"></property>
	</bean>
	
	<bean id="student2" class="com.pradip.spring.autowiredandqualifier.Student">
	<property name="rollno" value="50"></property>
	<property name="name" value="Abhijit"></property>
	<property name="address" value="Solapur"></property>
	</bean>
	
	</beans>
	
	
