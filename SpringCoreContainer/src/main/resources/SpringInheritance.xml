<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	
	
	
	<bean id="studentParent" class="com.pradip.spring.inheritance.Student" abstract="true"> 
	<property name="rollno" value="44"></property>
	</bean>
	
	
	
	<bean id="studentBean" parent="studentParent">
	<property name="name" value="Pradip"></property>
	<property name="address" value="Pune"></property>
	</bean>
	
	
	</beans>